{
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-class-properties"
  ],
  "presets": [
    ["@babel/env", { "shippedProposals": true }],
    [
      "@babel/react",
      { "runtime": "automatic", "importSource": "@emotion/react" }
    ],
    ["@babel/typescript", { "onlyRemoveTypeImports": true }]
  ]
}
